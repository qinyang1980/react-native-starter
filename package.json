{
  "name": "react-native-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tsc:watch": "tsc --watch --skipLibCheck",
    "tsc": "tsc --skipLibCheck",
    "start": "gulp start",
    "build": "gulp build",
    "test": "gulp test",
    "run-ios": "gulp run-ios",
    "run-android": "gulp run-android"
  },
  "dependencies": {
    "antd-mobile": "^2.1.1",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "react": "^16.2.0",
    "react-native": "^0.51.0",
    "styled-components": "^2.2.4"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.5",
    "@types/jest": "^21.1.8",
    "@types/react": "^16.0.30",
    "@types/react-native": "^0.51.2",
    "@types/react-test-renderer": "^16.0.0",
    "babel-jest": "21.2.0",
    "babel-plugin-import": "^1.6.3",
    "babel-preset-react-native": "4.0.0",
    "del": "^3.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "fs-extra": "^5.0.0",
    "gulp": "^3.9.1",
    "gulp-shell": "^0.6.3",
    "gulp-tslint": "^8.1.2",
    "jest": "21.2.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^21.2.4",
    "tslib": "^1.8.1",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2",
    "typescript-styled-plugin": "^0.2.2"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/setupTests.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
